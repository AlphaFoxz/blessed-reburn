{
  "include": [
    "src/*",
    "src/**/*"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "#core": [
        "../core/src"
      ],
      "#core/*": [
        "../core/src/*"
      ],
      "#cli": [
        "../cli/src"
      ],
      "#cli/*": [
        "../cli/src/*"
      ]
    }
  }
}