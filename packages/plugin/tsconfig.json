{
  "include": [
    "src/*",
    "src/**/*"
  ],
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "baseUrl": "./",
    "paths": {
      "#core": [
        "../core/src"
      ],
      "#core/*": [
        "../core/src/*"
      ],
    }
  }
}